{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": [
		"nette"
	],
	"type": "project",
	"license": [
		"MIT",
		"BSD-3-Clause",
		"GPL-2.0",
		"GPL-3.0"
	],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.1",
		"nette/di": "^3.1",
		"nette/forms": "^3.1",
		"nette/http": "^3.2",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.1",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.9",
		"vlucas/phpdotenv": "^5.6",
		"contributte/messenger": "^0.1.0",
		"contributte/di": "^0.5.6",
		"robmorgan/phinx": "^0.15.4"
	},
	"require-dev": {
		"nette/tester": "^2.4",
		"symfony/thanks": "^1",
		"phpstan/phpstan-nette": "^1.2",
		"phpmd/phpmd": "^2.14",
		"symplify/easy-coding-standard": "^12.0",
		"friendsofphp/php-cs-fixer": "^3.38",
		"squizlabs/php_codesniffer": "^3.7",
		"rector/rector": "^0.18.10"
	},
	"scripts": {
		"checks": [
			"@phpstan",
			"@ecs",
			"@phpmd"
		],
		"phpstan": "vendor/bin/phpstan analyse --memory-limit 1G",
		"rector": "vendor/bin/rector process --dry-run",
		"rectorf": "vendor/bin/rector process",
		"ecs": "vendor/bin/ecs check src",
		"ecsf": "vendor/bin/ecs check src --fix",
		"phpmd": "vendor/bin/phpmd src ansi phpmd-ruleset.xml"
	},
	"autoload": {
		"psr-4": {
			"Ondra\\App\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
